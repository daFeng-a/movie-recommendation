<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhang.mapper.MovieMapper">
    <select id="selectAll" resultType="com.zhang.entity.Movie">
        SELECT * FROM movies
    </select>
    <select id="selectById" resultType="com.zhang.entity.Movie">
        SELECT * FROM movies WHERE id = #{id}
    </select>
</mapper>